#**************************************************************
# Time Information
#**************************************************************

set_time_format -unit ns -decimal_places 3



#**************************************************************
# Create Clock
#**************************************************************

create_clock -name {clk} -period 20.000 -waveform { 0.000 10.000 } [get_ports {clk}]


#**************************************************************
# Create Generated Clock
#**************************************************************



#**************************************************************
# Set Clock Latency
#**************************************************************



#**************************************************************
# Set Clock Uncertainty
#**************************************************************

set_clock_uncertainty -setup -from [get_clocks clk] -to [get_clocks clk] 0.020 
set_clock_uncertainty -hold -from [get_clocks clk] -to [get_clocks clk] 0.020 

#**************************************************************
# Set Input Delay
#**************************************************************

set_input_delay -clock clk -max 1.532 [get_ports clk]
set_input_delay -clock key1 -max 2.540 [get_ports key1]
set_input_delay -clock rst_l -max 2.540 [get_ports rst_l]

#**************************************************************
# Set Output Delay
#**************************************************************

set_output_delay -clock buzzer -max  0.880 [get_ports buzzer]


